{
  "name": "bytenexus",
  "version": "1.0.0",
  "description": "Portfolio de desenvolvedor web especializado em criação de sites profissionais, responsivos e modernos",
  "main": "index.html",
  "scripts": {
    "dev": "python3 server.py",
    "build": "npm run minify:html && npm run minify:css && npm run minify:js",
    "minify:html": "html-minifier-terser --collapse-whitespace --remove-comments --minify-css --minify-js *.html -o dist/",
    "minify:css": "cleancss -o dist/assets/css/main.min.css assets/css/main.css",
    "minify:js": "terser assets/js/main.js -o dist/assets/js/main.min.js",
    "lighthouse": "lighthouse http://localhost:8000 --output html --output-path ./lighthouse-report.html",
    "validate": "html-validate *.html"
  },
  "keywords": [
    "desenvolvedor de sites",
    "criação de sites",
    "desenvolvimento web",
    "sites responsivos",
    "programador web",
    "freelancer web",
    "portfolio desenvolvedor",
    "HTML CSS JavaScript",
    "sites profissionais",
    "desenvolvedor frontend"
  ],
  "author": "Seu Nome",
  "license": "MIT",
  "devDependencies": {
    "html-minifier-terser": "^7.2.0",
    "clean-css-cli": "^5.6.2",
    "terser": "^5.16.6",
    "lighthouse": "^10.0.0",
    "html-validate": "^7.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/username/site-bytenexus.git"
  },
  "homepage": "https://username.github.io/site-bytenexus"
}